{
  "name": "svrnm/exceldatatables",
  "description": "Replace a worksheet within an Excel workbook (.xlsx) without changing any other properties of the file",
  "keywords": [
    "excel",
    "php",
    "datatables",
    "worksheet",
    "workbook",
    "xls",
    "spreadsheet"
  ],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Severin Neumann",
      "email": "severin.neumann@altmuehlnet.de"
    }
  ],
  "require": {
    "php": ">=8.0.0",
    "open-telemetry/api": "^1.0",
    "open-telemetry/sem-conv": "^1.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~8.0.x-dev",
    "mikey179/vfsstream": "~1.6",
    "phpbench/phpbench": "^1.3",
    "open-telemetry/sdk": "^1.0",
    "open-telemetry/exporter-otlp": "^1.0",
    "symfony/http-client": "8.0.x-dev",
    "nyholm/psr7": "^1.8@dev"
  },
  "autoload": {
    "psr-0": {
      "Svrnm\\ExcelDataTables": "src/"
    }
  },
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "tbachert/spi": true
    }
  }
}
